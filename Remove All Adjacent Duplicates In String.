#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

void removeadjacentduplicates(char*s){
    int length=strlen(s);
    char stack[length+1];
    int top=-1;
    for(int i=0;i<length;i++){
if(top>=0 && stack[top]==s[i]){
    top--;
    
}else{
stack[++top]=s[i];
    }
}
    stack[top+1]='\0';
    printf("%s\n",stack);
        
    }
int main(){
    char s[1000001];
    scanf("%s\n",s);
    removeadjacentduplicates(s);

    return 0;
}
